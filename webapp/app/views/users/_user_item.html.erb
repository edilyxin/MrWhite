<tr>
	<td><%= user_item_counter+1 %></td>
	<td><%= user_item.name %></td>
	<td><%= user_item.email %></td>
	<td><%= user_item.openid %></td>
	<td><%= user_item.tel %></td>
	<td><%= user_item.my_score%></td>
	<td>
		<%= link_to '编辑', edit_user_path(user_item) %>
